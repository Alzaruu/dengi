<Window x:Class="УчетБюдж.TYPEWindow1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:УчетБюдж"
        mc:Ignorable="d"
        Title="TYPEWindow1" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions> 
        <TextBlock Text="Введите новый тип записи" Grid.Row="0" FontSize="40" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <TextBox x:Name="TypeText" Grid.Row="1" FontSize="20" Margin="61,10,78,39"/>
        <Button x:Name="DobType" Grid.Row="2" FontSize="25" Content="Добавить новый тип" Margin="61,10,54,27" Click="DobType_Click"/>
    </Grid>
</Window>
